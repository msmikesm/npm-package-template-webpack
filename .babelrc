{
    "presets": [
        ["@babel/preset-env",
            {"targets": {
                "browsers": [
                    "last 3 versions",
                    "safari >= 7",
                    "ie >= 8"
                ],
                "node": "6.10"
            }}
        ]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}